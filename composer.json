{
    "name": "vlabs/amazon-mws-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 service wrapper for Amazon MWS PHP SDK",
    "keywords": ["amazon", "mws"],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Valentin Ferriere",
            "homepage": "http://www.v-labs.fr"
        }
    ],

    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": ">=2.1.x-dev",
        "choomz/amazon-mws-sdk": "dev-master"
    },
    
    "autoload": {
        "psr-0": { "Vlabs\\AmazonMWSBundle": "" }
    },

    "target-dir": "Vlabs/AmazonMWSBundle"
}